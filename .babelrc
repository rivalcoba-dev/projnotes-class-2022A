{
  "presets": [
    [
      "minify",{
        "builtIns": false,
        "evaluate": false,
        "mangle": false
      }
    ],
    [
      "@babel/preset-env",
      {
        "targets": { "node": "current" }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": "./",
        "alias": {
          "@s": "./server"
        }
      }
    ]
  ]
}
