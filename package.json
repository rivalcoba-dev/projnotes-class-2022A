{
  "name": "projnotes-2022a",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node ./dist/bin/www",
    "dev": "NODE_ENV=development babel-watch ./server/bin/www",
    "client:server": "NODE_ENV=development webpack serve --config ./webpack.dev.config.js --mode development",
    "clean:server": "rm -rf dist",
    "clean:client": "rm -rf public/javascripts/bundle.js && rm -rf public/stylesheets/styles.css",
    "build:s": "npm run clean:server && babel ./server --out-dir dist --copy-files",
    "build:client": "npm run clean:client && webpack --config ./webpack.config.js --mode production"
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "express-handlebars": "^5.3.2",
    "hbs": "^4.1.1",
    "http-errors": "~1.6.3",
    "mongoose": "^6.0.5",
    "morgan": "~1.9.1",
    "rimraf": "^3.0.2",
    "winston": "^3.3.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.13.15",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-minify": "^0.5.1",
    "babel-watch": "^7.4.1",
    "core-js": "^3.11.1",
    "css-loader": "^5.2.4",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-webpack-plugin": "^3.1.1",
    "mini-css-extract-plugin": "^1.6.0",
    "prettier": "^2.4.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-middleware": "^4.3.0",
    "webpack-dev-server": "^4.7.4",
    "webpack-hot-middleware": "^2.25.1"
  }
}
